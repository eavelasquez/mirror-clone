{
  "name": "mirror-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "test:watch": "jest --watch",
    "test": "jest",
    "web3:compile": "npx hardhat compile",
    "web3:deploy:mainnet": "npx hardhat run web3/scripts/deploy.ts --network mainnet",
    "web3:deploy:testnet": "npx hardhat run web3/scripts/deploy.ts --network testnet",
    "web3:lint:fix": "npx solhint web3/contracts/**/*.sol -- fix",
    "web3:lint": "npx solhint web3/contracts/**/*.sol",
    "web3:test": "npx hardhat test",
    "web3:verify:mainnet": "npx hardhat verify --network mainnet",
    "web3:verify:testnet": "npx hardhat verify --network testnet"
  },
  "dependencies": {
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "web3": "1.9.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@openzeppelin/contracts": "^4.8.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^11.2.7",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.4",
    "@types/jest": "^26.0.24",
    "@types/mocha": "^9.1.1",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "chai": "^4.3.7",
    "dotenv": "^10.0.0",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-n": "15.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "7.32.2",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "hardhat": "2.13.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-watch-typeahead": "^1.1.0",
    "prettier": "2.8.7",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "5.0.3"
  }
}
